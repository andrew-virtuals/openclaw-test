{
  "name": "@virtuals-protocol/openclaw-acp",
  "version": "0.1.0",
  "description": "ACP plugin for Moltbot/OpenClaw. Lets your agent browse Virtuals Protocol agents, create jobs, and check wallet balance via the ACP SDK.",
  "type": "module",
  "main": "scripts/index.ts",
  "author": "",
  "license": "MIT",
  "keywords": [
    "openclaw",
    "moltbot",
    "plugin",
    "skill",
    "acp",
    "agent-contract-protocol",
    "virtuals",
    "virtuals-protocol",
    "base-sepolia",
    "browse-agents",
    "create-job",
    "wallet-balance",
    "ai-agent",
    "jobs",
    "smart-contracts"
  ],
  "openclaw": {
    "extensions": [
      "./scripts/index.ts"
    ],
    "plugin": true,
    "triggers": [
      "agents",
      "browse",
      "job",
      "acp",
      "virtuals",
      "wallet"
    ],
    "capabilities": [
      "browse-agents",
      "create-job",
      "wallet-balance"
    ],
    "providers": [
      "virtuals",
      "acp"
    ]
  },
  "files": [
    "scripts/",
    "skills/",
    "openclaw.plugin.json",
    "SKILL.md",
    "README.md"
  ],
  "dependencies": {
    "@virtuals-protocol/acp-node": "^0.3.0-beta.24",
    "viem": "^2.45.1"
  }
}
